        <TargetFramework>net5.0</TargetFramework>
        <AssemblyName>CottageTemperature.Libraries.Telegram</AssemblyName>
        <RootNamespace>CottageTemperature.Libraries.Telegram</RootNamespace>
        <Nullable>enable</Nullable>
    </PropertyGroup>

    <PropertyGroup Condition=" '$(Configuration)' == 'Debug' ">
      <DocumentationFile>bin\Debug\Libraries.Telegram.xml</DocumentationFile>
